generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  directUrl         = env("DIRECT_URL")
}

model Projects {
  id                 String   @id
  Completion_date    String?
  Year               String?    
  Quarter            String?  
  Month              String?  
  TwoBedroom         Boolean
  SixBedroom         Boolean
  OneBedroomPool     Boolean
  OneBedroom         Boolean
  FourBedroom        Boolean
  ThreeBedroom       Boolean
  MergedStudios      Boolean
  TwoBedroomPool     Boolean
  EightBedroom       Boolean
  NineBedroom        Boolean
  SevenBedroom       Boolean
  TwoBedroomPlusPool Boolean
  StudioPool         Boolean
  FiveBedroom        Boolean
  ThreeBedroomPool   Boolean
  TwoAndHalfBedroom  Boolean
  Studio             Boolean
  OneAndHalfBedroom  Boolean
  ThreeAndHalfBedroom Boolean
  Coordinates        String?
  Project_name       String?
  Developers_name    String?
  Area_name          String?
  Region             String?
  Publish            String?
  Status             String?
  Priority           String?
  Floors             String?
  Furnishing         String?
  Price_from_AED     String?    
  Price_to_AED       String?   
  Cover_URL          String?  
}